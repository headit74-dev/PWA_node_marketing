<script>
    // $(document).ready(function () {
    //     $('#article').hide();
    // })
    $(document).ready(function() {
        $('#article').hide();
        var owl = $('.owl-carousel');
        owl.owlCarousel({
            loop: false,
            nav: true,
            margin: 10,
            responsive: {
                0: {
                    items: 1
                },
                960: {
                    items: 2
                },
                1100: {
                    items: 3
                }
            }
        });
        owl.on('mousewheel', '.owl-stage', function(e) {
            if (e.deltaY > 0) {
                owl.trigger('next.owl');
            } else {
                owl.trigger('prev.owl');
            }
            e.preventDefault();
        });
    })
</script>
